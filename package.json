  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.9.0",
    "c8": "^10.1.2",
    "eslint": "^9.15.0",
    "glob": "^11.0.0",
    "mocha": "^10.8.2",
    "rollup": "^4.27.2",
    "rollup-plugin-cleandir": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "keywords": [

{
  "name": "cwt",
  "version": "1.0.0",
  "description": "An implementation of the cwt standard RFC8392",
  "main": "./dist/cwt.cjs",
  "module": "./dist/cwt.mjs",
  "types": "./dist/cwt.d.ts",
